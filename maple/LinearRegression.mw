<Group hide-input="false" labelreference="L4" drawlabel="false" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Use Plate to remove duplication of Gaussian(0,10) in the prior distribution
